{
  "name": "interpolate-es",
  "version": "0.0.0",
  "description": "Create tagged templates dynamically",
  "license": "MIT",
  "author": "Evan Plaice <evanplaice@gmail.com> (http://evanplaice.com/)",
  "repository": "http://github.com/vanillaes/interpolate-es/",
  "main": "src/interpolate.js",
  "type": "module",
  "scripts": {
    "test": "tape-es",
    "test:watch": "tape-watch-es",
    "lint": "semistandard",
    "types": "npx tsc -p .config/jsconfig.json",
    "build:typings": "npx tsc -p .config/tsconfig.json",
    "package": "npx rimraf package && npm pack | tail -n 1 | xargs tar -xf",
    "preversion": "npm test && npm run lint && npm run types",
    "version": "npm run build:typings",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "semistandard": "^14.2.0",
    "tape-es": "^1.1.1"
  }
}
